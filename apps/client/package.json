{
  "name": "client",
  "version": "0.0.0",
  "dependencies": {
    "@lightest/core": "workspace:*",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/vue-query": "^5.83.0",
    "@ts-rest/core": "^3.52.1",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "fp-ts": "^2.16.10",
    "lucide-vue-next": "^0.525.0",
    "pinia": "^3.0.3",
    "reka-ui": "^2.3.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.17",
    "vue-i18n": "^11.1.10",
    "vue-router": "^4.5.1",
    "vue-sonner": "^2.0.1",
    "zod": "^3.25.36"
  },
  "devDependencies": {
    "@feature-sliced/steiger-plugin": "^0.5.6",
    "@lightest/eslint-config": "workspace:*",
    "@lightest/lint-staged-config": "workspace:*",
    "@lightest/prettier-config": "workspace:*",
    "@lightest/typescript-config": "workspace:*",
    "@prettier/plugin-xml": "^3.4.1",
    "@tanstack/eslint-plugin-query": "^5.81.2",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.0.1",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.1",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.29.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-vue": "~10.2.0",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "steiger": "^0.5.7",
    "stylelint": "^16.21.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-vue-devtools": "^7.7.7",
    "vue-tsc": "^2.2.10"
  },
  "private": true,
  "scripts": {
    "build": "run-p type-check \"build-only {@}\" --",
    "build-only": "vite build",
    "dev": "vite",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "lint:check": "eslint . && stylelint '**/*.{css,vue}'",
    "lint:fix": "eslint --fix . && stylelint --fix '**/*.{css,vue}'",
    "preview": "vite preview",
    "type-check": "concurrently -p \"[{name}]\" -n \"App,Node\" -c \"blue.bold,yellow.bold\" \"pnpm type-check-app\" \"pnpm type-check-node\" \"pnpm type-check-ui\"",
    "type-check-app": "vue-tsc --noEmit -p tsconfig.app.json",
    "type-check-node": "vue-tsc --noEmit -p tsconfig.node.json",
    "type-check-ui": "vue-tsc --noEmit -p tsconfig.ui.json"
  },
  "type": "module"
}
