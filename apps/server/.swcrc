{
  "$schema": "https://swc.rs/schema.json",
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true
    },
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "target": "esnext"
  },
  "module": {
    "type": "es6",
    "resolveFully": true
  },
  "sourceMaps": true
}
